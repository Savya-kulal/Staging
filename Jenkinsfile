pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo"this is build stage"'
            }
        }
        stage('Test') {
            steps {
                'echo"this is test stage"'
            }
        }
        stage('Deploy') {
            steps {
                'echo"this is deploy stage"'
            }
        }
    }
}
